{
  "name": "@cedros/login-react",
  "version": "0.0.1",
  "description": "React component library for authentication with email/password, Google OAuth, and Solana wallet sign-in",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./email-only": {
      "import": {
        "types": "./dist/email-only.d.ts",
        "default": "./dist/email-only.js"
      },
      "require": {
        "types": "./dist/email-only.d.ts",
        "default": "./dist/email-only.cjs"
      }
    },
    "./google-only": {
      "import": {
        "types": "./dist/google-only.d.ts",
        "default": "./dist/google-only.js"
      },
      "require": {
        "types": "./dist/google-only.d.ts",
        "default": "./dist/google-only.cjs"
      }
    },
    "./solana-only": {
      "import": {
        "types": "./dist/solana-only.d.ts",
        "default": "./dist/solana-only.js"
      },
      "require": {
        "types": "./dist/solana-only.d.ts",
        "default": "./dist/solana-only.cjs"
      }
    },
    "./style.css": "./dist/login-react.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"stories/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "release": "npm run prepublishOnly && npm publish --access public"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@solana/wallet-adapter-base": {
      "optional": true
    },
    "@solana/wallet-adapter-react": {
      "optional": true
    },
    "@solana/wallet-adapter-react-ui": {
      "optional": true
    },
    "@solana/wallet-adapter-wallets": {
      "optional": true
    },
    "@solana/web3.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-solflare": "^0.6.32",
    "@solana/web3.js": "^1.98.4",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^26.1.0",
    "msw": "^2.12.4",
    "postcss": "^8.5.6",
    "prettier": "^3.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.1.11",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "uuid": "^13.0.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.3"
  },
  "keywords": [
    "react",
    "authentication",
    "login",
    "google-oauth",
    "solana",
    "wallet",
    "cedros"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conorholds/cedros-login.git",
    "directory": "ui"
  },
  "bugs": {
    "url": "https://github.com/conorholds/cedros-login/issues"
  },
  "homepage": "https://github.com/conorholds/cedros-login#readme",
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^1.7.2",
    "@scure/bip39": "^1.6.0",
    "hash-wasm": "^4.12.0",
    "qrcode": "^1.5.4",
    "secrets.js-grempe": "^2.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
