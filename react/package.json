{
  "name": "@cedros/login-react",
  "version": "0.0.17",
  "description": "React component library for authentication with email/password, Google OAuth, and Solana wallet sign-in",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./admin-only": {
      "import": {
        "types": "./dist/admin-only.d.ts",
        "default": "./dist/admin-only.js"
      },
      "require": {
        "types": "./dist/admin-only.d.ts",
        "default": "./dist/admin-only.cjs"
      }
    },
    "./email-only": {
      "import": {
        "types": "./dist/email-only.d.ts",
        "default": "./dist/email-only.js"
      },
      "require": {
        "types": "./dist/email-only.d.ts",
        "default": "./dist/email-only.cjs"
      }
    },
    "./google-only": {
      "import": {
        "types": "./dist/google-only.d.ts",
        "default": "./dist/google-only.js"
      },
      "require": {
        "types": "./dist/google-only.d.ts",
        "default": "./dist/google-only.cjs"
      }
    },
    "./solana-only": {
      "import": {
        "types": "./dist/solana-only.d.ts",
        "default": "./dist/solana-only.js"
      },
      "require": {
        "types": "./dist/solana-only.d.ts",
        "default": "./dist/solana-only.cjs"
      }
    },
    "./style.css": "./dist/login-react.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@solana/wallet-adapter-base": {
      "optional": true
    },
    "@solana/wallet-adapter-react": {
      "optional": true
    },
    "@solana/wallet-adapter-react-ui": {
      "optional": true
    },
    "@solana/wallet-adapter-wallets": {
      "optional": true
    },
    "@solana/web3.js": {
      "optional": true
    },
    "@solana-mobile/wallet-standard-mobile": {
      "optional": true
    }
  },
  "keywords": [
    "react",
    "authentication",
    "login",
    "google-oauth",
    "solana",
    "wallet",
    "cedros"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conorholds/cedros-login.git",
    "directory": "ui"
  },
  "bugs": {
    "url": "https://github.com/conorholds/cedros-login/issues"
  },
  "homepage": "https://github.com/conorholds/cedros-login#readme",
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^1.7.2",
    "@scure/bip39": "^1.6.0",
    "hash-wasm": "^4.12.0",
    "qrcode": "^1.5.4",
    "secrets.js-grempe": "^2.0.0"
  }
}
